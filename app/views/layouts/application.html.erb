<!DOCTYPE html>
<html>
    <head>
        <title>Tadlstats</title>
        <%= csrf_meta_tags %>
        <%= csp_meta_tag %>

        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
        <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
        <%= stylesheet_link_tag href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" %>
    </head>

    <body class="fixed skin-blue-light sidebar-collapse">
        <div class="wrapper">
            <%= render 'layouts/navigation/layout' %>
            <div class="content-wrapper">
                <div style="padding: 5px;">
                    <%= yield %>
                </div>
            </div>
        </div>
    </body>
</html>
